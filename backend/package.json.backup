{
  "name": "ai-question-generator-backend",
  "version": "1.0.0",
  "description": "AI题目生成器后端服务",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
      "dev": "tsx watch src/server.ts",
      "build": "tsc",
      "start": "node dist/server.js",

      "migrate:latest": "knex migrate:latest --knexfile knexfile.ts",
      "migrate:rollback": "knex migrate:rollback --knexfile knexfile.ts",
      "migrate:make": "knex migrate:make --knexfile knexfile.ts",
      "migrate:status": "knex migrate:status --knexfile knexfile.ts",
      "migrate:up": "knex migrate:up --knexfile knexfile.ts",
      "migrate:down": "knex migrate:down --knexfile knexfile.ts",

      "migrate:run": "npm run build && knex migrate:latest --knexfile knexfile.ts",
      "migrate:check": "knex migrate:status --knexfile knexfile.ts",

      "migrate:dev": "NODE_ENV=development knex migrate:latest --knexfile knexfile.ts",
      "migrate:dev:rollback": "NODE_ENV=development knex migrate:rollback --knexfile knexfile.ts",
      "migrate:dev:status": "NODE_ENV=development knex migrate:status --knexfile knexfile.ts",

      "migrate:prod": "NODE_ENV=production npm run build && knex migrate:latest --knexfile knexfile.ts",
      "migrate:prod:status": "NODE_ENV=production knex migrate:status --knexfile knexfile.ts",

      "seed:run": "knex seed:run --knexfile knexfile.ts",
      "seed:make": "knex seed:make --knexfile knexfile.ts",

      "postbuild": "rm -f dist/migrations/*.d.ts && rm -f dist/seeds/*.d.ts",

      "test": "jest",
      "test:services": "node scripts/test-services.js",
      "test:db": "npm run build && node scripts/test-database-tables.js",
      "check:env": "node scripts/check-env.js",
      "lint": "eslint src --ext .ts",
      "format": "prettier --write src/**/*.ts",
      "docker:setup": "chmod +x scripts/setup-docker.sh && ./scripts/setup-docker.sh",
      "docker:up": "docker-compose up -d",
      "docker:down": "docker-compose down",
      "docker:logs": "docker-compose logs -f"
    },
  "keywords": [
    "ai",
    "education",
    "fastify",
    "typescript",
    "rag",
    "vector-database"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^8.3.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@qdrant/js-client-rest": "^1.8.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.6.1",
    "fastify": "^4.29.1",
    "knex": "^3.1.0",
    "minio": "^7.1.3",
    "mysql2": "^3.14.2",
    "openai": "^4.28.4",
    "redis": "^4.6.12",
    "tiktoken": "^1.0.10",
    "uuid": "^9.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.19.7",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
